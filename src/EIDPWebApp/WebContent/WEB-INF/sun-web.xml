<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE sun-web-app PUBLIC "-//Sun Microsystems, Inc.//DTD Application Server 8.1 Servlet 2.4//EN" "http://www.sun.com/software/appserver/dtds/sun-web-app_2_4-1.dtd">
<sun-web-app>
  <context-root>/EIDPWebApp</context-root>
  <ejb-ref>
    <ejb-ref-name>DBMapping</ejb-ref-name>
    <jndi-name>ejb/DBMapping</jndi-name>
  </ejb-ref>
  <ejb-ref>
    <ejb-ref-name>EIDPWebAppCache</ejb-ref-name>
    <jndi-name>ejb/TwWebAppCache</jndi-name>
  </ejb-ref>
  <locale-charset-info>
      <locale-charset-map locale="" charset=""/>
      <parameter-encoding default-charset="UTF-8"/>
  </locale-charset-info>
</sun-web-app>
